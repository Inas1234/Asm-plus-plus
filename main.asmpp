global _start

section .data
    msg: "Hello World\n"

fn print(message){
    mov rax, 1
    mov rdi, 1
    mov rsi, message
    mov rdx, 12
    syscall
}

fn exit(code) {
    mov rax, 60
    mov rdi, code
    syscall
}

fn _start() 
{
    mov rax, 30
    mov rdi, 30
    if (rax eq rdi) { 
        mov rax, 1
        mov rdi, 2
        if (rax gt rdi) {
            call print(msg)
        }
        call print(msg)
    }


    call exit(0)
}
